{
  "name": "have-a-cookie",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build",
    "serve": "preact build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src",
    "publish": "preact build && aws s3 cp build/ s3://have-a-cookie/ --recursive"
  },
  "eslintIgnore": [
    "build/*"
  ],
  "devDependencies": {
    "d3": "^4.11.0",
    "eslint": "^4.5.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-react": "^7.5.0",
    "if-env": "^1.0.0",
    "preact-cli": "^2.0.0"
  },
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.1",
    "debounce": "^1.1.0",
    "deep-equal": "^1.0.1",
    "preact": "^8.2.1",
    "preact-compat": "^3.17.0",
    "preact-material-components": "^1.2.4",
    "react-faux-dom": "^4.1.0"
  }
}
